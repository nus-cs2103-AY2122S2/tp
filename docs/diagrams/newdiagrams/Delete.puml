@startuml
!include Style.puml
box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":DeleteCommandParser" as DeleteCommandParser LOGIC_COLOR
participant "d:DeleteCommand" as DeleteCommand LOGIC_COLOR
end box

box Model
participant ":Model" as Model LOGIC_COLOR_T4
participant ":AddressBook" as AddressBook LOGIC_COLOR_T4
end box

[-> LogicManager : execute(delete)
activate LogicManager

LogicManager -> DeleteCommandParser : parseCommand(delete)
activate DeleteCommandParser

create DeleteCommand
DeleteCommandParser -> DeleteCommand
activate DeleteCommand

DeleteCommand --> DeleteCommandParser
deactivate DeleteCommand

DeleteCommandParser --> LogicManager : d
deactivate DeleteCommandParser

LogicManager -> DeleteCommand : execute(model)
activate DeleteCommand

DeleteCommand -> Model : deleteLineup()
activate Model

Model -> AddressBook : deleteLineup()
activate AddressBook

AddressBook --> Model
deactivate AddressBook

Model --> DeleteCommand
deactivate Model

DeleteCommand --> LogicManager : result
deactivate DeleteCommand
@enduml
