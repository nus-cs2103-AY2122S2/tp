<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>
<fx:root resizable="true" title="Reminders" minWidth="400" minHeight="600" type="javafx.stage.Stage"
         xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/bell_icon.png"/>
  </icons>
  <scene>
    <Scene>
      <stylesheets>
        <URL value="@DarkTheme.css"/>
      </stylesheets>
      <VBox styleClass="pane-with-border" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
        <StackPane fx:id="reminderDateListPanelPlaceholder" VBox.vgrow="ALWAYS"/>
      </VBox>
    </Scene>
  </scene>
</fx:root>
