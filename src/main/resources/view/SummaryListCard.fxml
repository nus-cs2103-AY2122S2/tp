<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<HBox id="cardPane" fx:id="cardPane" prefWidth="150.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane prefHeight="0.0" prefWidth="150.0" HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <children>
         <VBox alignment="CENTER_LEFT" prefHeight="85.0" prefWidth="150.0">
            <children>
          <Label fx:id="name" styleClass="cell_big_label" text="\$name">
            <minWidth>
              <!-- Ensures that the label text is never truncated -->
              <Region fx:constant="USE_PREF_SIZE" />
            </minWidth>
          </Label>
               <HBox prefHeight="100.0" prefWidth="200.0">
                  <children>
                <Label fx:id="currApplications" styleClass="cell_big_label" text="\$first" />
                     <Label fx:id="totalApplications" styleClass="cell_big_label" text="\$first" />
                  </children>
               </HBox>
               <Label fx:id="id1" styleClass="cell_big_label" text="Applications">
                  <minWidth>
                     <Region fx:constant="USE_PREF_SIZE" />
                  </minWidth>
               </Label>
            </children>
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding></VBox>
    </children>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
  </GridPane>
</HBox>
