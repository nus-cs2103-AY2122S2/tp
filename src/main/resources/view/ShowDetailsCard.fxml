<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_LEFT" minHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <TextArea fx:id="name" editable="false" focusTraversable="false" prefRowCount="1" styleClass="cell_big_label_details" text="\\\title" wrapText="true" />
<FlowPane fx:id="tags">
      <VBox.margin>
         <Insets left="5.0" right="5.0" />
      </VBox.margin></FlowPane>
<Label fx:id="status" focusTraversable="false" styleClass="cell_small_label_details" text="\$status" VBox.vgrow="NEVER">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin></Label>
   <FlowPane fx:id="ratings" maxHeight="40.0" minWidth="40.0" prefHeight="40.0" />
   <TextArea fx:id="comment" editable="false" focusTraversable="false" minHeight="-Infinity" prefHeight="200.0" prefRowCount="1" styleClass="cell_small_textfield" text="\\\comment" wrapText="true">
      <VBox.margin>
         <Insets left="5.0" right="5.0" />
      </VBox.margin>
   </TextArea>
   <Pane VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets left="5.0" right="5.0" />
      </VBox.margin></Pane>
   <padding>
      <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
   </padding>
</VBox>
