<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>

<?import javafx.stage.Stage?>
<?import javafx.scene.image.Image?>
<?import java.net.URL?>
<?import javafx.scene.chart.PieChart?>
<Stage minHeight="600" minWidth="450" title="Statistics window" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <icons>
        <Image url="@/images/address_book_32.png" />
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@DarkTheme.css" />
                <URL value="@Extensions.css" />
            </stylesheets>
            <VBox>
                <children>
                    <PieChart layoutX="-25.0" layoutY="107.0" prefHeight="386.0" prefWidth="500.0" title="Popular areas for buyers and sellers" fx:id="pieChart"/>
                </children>
            </VBox>
        </Scene>
    </scene>
</Stage>
